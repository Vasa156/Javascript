function compareNumbers(){
   let a = prompt("Enter A number : ");
   let b = prompt("Enter B number : ");
    a = Number(a);
    b = Number(b);
    if(a>b){
        console.log("A is Greater");
    }
    else if(a<b){
        console.log("B is Greater");
    }
    else{
        console.log("Both are same!");
    }
}
compareNumbers();
