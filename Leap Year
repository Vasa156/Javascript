function leapYear(){
    let year = Number(prompt("Enter a Year : "));
    
    if(year % 400 === 0){
        return "Leap Year";
    }
    else if(year % 100 === 0){
        return "Not a Leap Year";
    }
    else if(year % 4 === 0){
        return "Leap Year";
    }
    else{
        return "Not a Leap Year";
    }
}

let result = leapYear();
console.log("Result : ", result);
